generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type MangaNameAnimation {
  animeId             String
  anime_imageCarousel MangaNameAnimationAnimeImageCarousel[]
  anime_imageTop      String
  anime_titre         String
  backNav             String
  classNav            String
  imageSee            MangaNameAnimationImageSee[]
}

type MangaNameAnimationAnimeImageCarousel {
  anime_href  String
  anime_image String
  anime_name  String
}

type MangaNameAnimationImageSee {
  href   String
  images String
}

type MangaNameImageCarousel {
  image String
  name  String
  url   String
}

type MangaNameImageShow {
  img String
  url String
}

model mangaName {
  id            String                   @id @default(auto()) @map("_id") @db.ObjectId
  animation     MangaNameAnimation[]
  back          String
  imageCarousel MangaNameImageCarousel[]
  imageShow     MangaNameImageShow[]
  imageTop      String
  navClass      String
  slug          String                   @unique
  titre         String
}
